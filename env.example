# Snowflake Configuration
SNOWFLAKE_USER=your_snowflake_user
SNOWFLAKE_ACCOUNT=your_account_id
SNOWFLAKE_WAREHOUSE=your_warehouse_name
SNOWFLAKE_DATABASE=tpcds_performance_test
SNOWFLAKE_ROLE=ACCOUNTADMIN

# Snowflake Schema Names
SNOWFLAKE_SCHEMA_NATIVE=tpcds_native_format
SNOWFLAKE_SCHEMA_ICEBERG_SF=tpcds_iceberg_sf_format
SNOWFLAKE_SCHEMA_ICEBERG_GLUE=tpcds_iceberg_glue_format
SNOWFLAKE_SCHEMA_EXTERNAL=tpcds_external_format

# Snowflake Authentication
SNOWFLAKE_PRIVATE_KEY_FILE=/path/to/rsa_key.p8
SNOWFLAKE_PRIVATE_KEY_PASSPHRASE=

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your_aws_account_id

# AWS S3 Configuration
AWS_S3_BUCKET=your-iceberg-bucket-name
AWS_S3_ICEBERG_PREFIX=tcp-ds-data
AWS_S3_ICEBERG_GLUE_PATH=s3://your-iceberg-bucket-name/iceberg-performance-test/iceberg_glue_format/

# AWS Glue Configuration
AWS_GLUE_DATABASE=iceberg_performance_test
AWS_GLUE_CATALOG_ID=your_aws_account_id
AWS_GLUE_CATALOG_NAME=glue_catalog

# AWS IAM Configuration
AWS_ROLE_NAME=snowflake-iceberg-role
AWS_ROLE_ARN=arn:aws:iam::your_account_id:role/snowflake-iceberg-role

# Project Paths
PROJECT_ROOT=/path/to/iceberg-performance-benchmark
TPCDS_DATA_DIR=data/tpcds_data_sf0.1

